{
  "builds": [
    { "src": "app.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/app.py" }
  ],
  "env": {
    "PYTHONUNBUFFERED": "true",
    "FLASK_ENV": "production",
    "FLASK_APP": "app.py"
  },
  "functions": {
    "app": {
      "runtime": "vercel-python@3.9"
    }
  }
}
